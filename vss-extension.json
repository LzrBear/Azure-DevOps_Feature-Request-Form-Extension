{
    "manifestVersion": 1,
    "id": "feature-request-form-extension",
    "version": "0.1.0",
    "name": "Feature Request Form",
    "description": "An extension for adding a feature request form.",
    "publisher": "LzrBear",
    "categories": ["Azure Boards"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
            }
        ],
    "icons": {
        "default": "images/logo.png"
     },
    "contributions": [
        {
            "id": "Lzrbear.FeatureRequestForm",
            "type": "ms.vss-web.hub",
            "description": "Adds a 'Feature Request Form' page to the Work hub group.",
            "targets": [
                "ms.vss-work-web.work-hub-group"
                ],
            "properties": {
                "name": "Feature Request Form",
                "order": 99,
                "uri": "hello-world.html"
            }
        }
    ],
    "scopes": [
        "vso.work"
    ],
    "files": [
        {
            "path": "hello-world.html", "addressable": true
        },
        {
            "path": "sdk/scripts", "addressable": true
        },
        {
            "path": "images/logo.png", "addressable": true
        }
    ]
}