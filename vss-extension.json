{
    "manifestVersion": 1,
    "id": "feature-request-form",
    "publisher": "lzrbear",
    "version": "1.0.35",
    "name": "feature-request-form",
    "description": "Adds a simple form to help users generate features by taking in key data and auto-populating the other fields",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "build/logo512.png"
    },
    "contributions": [
        {
            "id": "new-feature",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-web.home-hub-group"
            ],
            "properties": {
                "name": "Feature Request Form",
                "uri": "build/index.html"
            }
        }
    ],
    "scopes": [
        "vso.work_write"
    ],
    "files": [
        {
            "path": "build",
            "addressable": true
        }
    ]
}